  "main": "./dist/nepali-react-datepicker.es.js",
  "module": "./dist/nepali-react-datepicker.umd.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
